                                            <td nowrap class="displayField" valign="top"><b> ${ _("Dates")}:</b></td>
                                            <td width="100%">${ dateInterval }</td>
                                        </tr>
                                        ${ conveners }
                                        ${ material }
                                        <tr>
                                            <td colspan="2">&nbsp;</td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>
${ contribs }
