msgstr "Láthatatlan"

#: indico/modules/categories/util.py
msgid "Everywhere"
msgstr "Mindenhol"

#: indico/modules/categories/views.py
msgid "Indico Atom feed"
msgstr "Indico Atom feed"

#: indico/modules/categories/templates/display/base.html
#: indico/modules/categories/views.py
msgid "Calendar"
msgstr "Naptár"

#: indico/modules/categories/controllers/admin.py
